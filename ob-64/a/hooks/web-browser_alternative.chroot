#!/bin/sh
# set iceweasel as default X web browser.
#############################################

set -e

echo STAR-LIVE-Hook: "$0"
echo ""

update-alternatives --install /usr/bin/x-www-browser x-www-browser /usr/bin/iceweasel 10
