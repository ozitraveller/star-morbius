label live-@FLAVOUR@-live
	menu label ^Live
	menu default
 	linux @LINUX@
 	initrd @INITRD@
 	append @APPEND_LIVE@
	
label live-@FLAVOUR@-toram
	menu label ^Live (toram)
 	linux @LINUX@
 	initrd @INITRD@
 	append @APPEND_LIVE@ toram	

label live-@FLAVOUR@-failsafe
	menu label ^Live (failsafe)
 	linux @LINUX@
 	initrd @INITRD@
 	append @APPEND_LIVE_FAILSAFE@
